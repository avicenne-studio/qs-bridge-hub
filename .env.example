# Must always set to production
# @see {@link https://www.youtube.com/watch?v=HMM7GJC5E2o}
NODE_ENV=production

# Server
FASTIFY_CLOSE_GRACE_DELAY=1000
LOG_LEVEL=info

# Security
RATE_LIMIT_MAX=4 # 4 is for tests, increase if you need more
POLLER_INTERVAL_MS=5000

# Database
SQLITE_DB_FILE=./data/hub.sqlite3
HOST=3000
PORT=0000

# Oracles
ORACLE_URLS=http://127.0.0.1:3001,http://127.0.0.1:3002,http://127.0.0.1:3003,http://127.0.0.1:3004,http://127.0.0.1:3005,http://127.0.0.1:3006
ORACLE_SIGNATURE_THRESHOLD=0.6
ORACLE_COUNT=6

# Hub signing
HUB_KEYS_FILE=./data/hub-keys.json

# Solana listener
SOLANA_WS_URL='wss://devnet.helius-rpc.com/?api-key=YOUR_API_KEY'
SOLANA_FALLBACK_WS_URL='wss://devnet.helius-rpc.com/?api-key=YOUR_API_KEY'
SOLANA_LISTENER_ENABLED=true

# Helius transaction poller
HELIUS_RPC_URL=https://devnet.helius-rpc.com/?api-key=YOUR_API_KEY
HELIUS_POLLER_ENABLED=true
HELIUS_POLLER_INTERVAL_MS=300000
HELIUS_POLLER_LOOKBACK_SECONDS=600
HELIUS_POLLER_TIMEOUT_MS=30000
HELIUS_POLLER_RETRY_DELAY_MS=1000
QUBIC_RPC_URL=http://127.0.0.1:3015/events
QUBIC_POLLER_ENABLED=false
QUBIC_POLLER_INTERVAL_MS=5000
QUBIC_POLLER_TIMEOUT_MS=5000

# Token
TOKEN_MINT=4bbjhGLSYwku6Y44dqwcroRfj2vHCdiHJ9SUmndc4FVg

# sqlite
data
